<p id="notice"><%= notice %></p>

<h1>Wreactions</h1>

<table>
  <thead>
    <tr>
      <th>Text</th>
      <th>Index</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @wreactions.each do |wreaction| %>
      <tr>
        <td><%= wreaction.text %></td>
        <td><%= wreaction.index %></td>
        <td><%= link_to 'Show', wreaction %></td>
        <td><%= link_to 'Edit', edit_wreaction_path(wreaction) %></td>
        <td><%= link_to 'Destroy', wreaction, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Wreaction', new_wreaction_path %>
