<p id="notice"><%= notice %></p>

<h1>Wallies</h1>

<table>
  <thead>
    <tr>
      <th>Version</th>
      <th>Description</th>
      <th>Current</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @wallies.each do |wally| %>
      <tr>
        <td><%= wally.version %></td>
        <td><%= wally.description %></td>
        <td><%= wally.current %></td>
        <td><%= link_to 'Show', wally %></td>
        <td><%= link_to 'Edit', edit_wally_path(wally) %></td>
        <td><%= link_to 'Destroy', wally, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Wally', new_wally_path %>
