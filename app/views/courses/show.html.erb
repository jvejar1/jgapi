<p id="notice"><%= notice %></p>
<p><%=@course.get_full_name+" "+@course.school.name unless @course.school.nil? %></p>
<p><%=@students_count %> alumnos</p>
<%= link_to 'Edit', edit_course_path(@course) %> |
<%= link_to 'Back', courses_path %>
<hr></hr>
<%= form_tag(upload_students_path(@course.id),{ :multipart => true}) do %>
