<p id="notice"><%= notice %></p>

<h1>Aces</h1>

<table>
  <thead>
    <tr>
      <th>Version</th>
      <th>Current</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @aces.each do |ace| %>
      <tr>
        <td><%= ace.version %></td>
        <td><%= ace.current %></td>
        <td><%= link_to 'Show', ace %></td>
        <td><%= link_to 'Edit', edit_ace_path(ace) %></td>
        <td><%= link_to 'Destroy', ace, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ace', new_ace_path %>
